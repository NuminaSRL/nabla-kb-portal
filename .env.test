# Test Environment Configuration
# Copy this file to .env.local for local testing

# Application URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Test User Credentials (for E2E tests)
TEST_FREE_USER_EMAIL=free-tier@example.com
TEST_FREE_USER_PASSWORD=FreeUser123!

TEST_PRO_USER_EMAIL=pro-tier@example.com
TEST_PRO_USER_PASSWORD=ProUser123!

TEST_ENTERPRISE_USER_EMAIL=enterprise-tier@example.com
TEST_ENTERPRISE_USER_PASSWORD=EnterpriseUser123!

# Test Configuration
TEST_TIMEOUT=30000
TEST_RETRY_COUNT=2
TEST_PARALLEL_WORKERS=4

# Feature Flags for Testing
ENABLE_MOCK_DATA=false
ENABLE_TEST_LOGGING=true
SKIP_AUTH_IN_TESTS=false
